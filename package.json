{
  "name": "vite-vue-js-starter-template",
  "description": "Vue3 JavaScript Starter Project For Vite.",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "ElanYoung",
    "email": "william@starimmortal.com",
    "url": "https://doc.starimmortal.com"
  },
  "homepage": "https://elanyoung.github.io/vite-vue-js-starter-template",
  "repository": {
    "type": "git",
    "url": "git@github.com:ElanYoung/vite-vue-js-starter-template.git"
  },
  "bugs": {
    "url": "https://github.com/ElanYoung/vite-vue-js-starter-template/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/ --fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "pinia": "^2.1.3",
    "tdesign-vue-next": "^1.3.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.6.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.2",
    "sass-loader": "^13.3.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-babel": "^1.1.3",
    "vue-eslint-parser": "^9.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,sass,scss,less}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "browserslist": [
    "defaults"
  ]
}
